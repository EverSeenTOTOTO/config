alias df='df -h'
alias ll="ls -Ahl"
alias rm="rm -rI"
alias mkdir='mkdir -p';
alias fd=fdfind
alias grep='grep --color=auto'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'

alias ..='cd ..'
alias ...='cd ../..'

alias path='echo -e ${PATH//:/\\n}'
alias venv="source ~/venv/bin/activate"
alias ngr='sudo nginx -t && sudo nginx -s reload'
