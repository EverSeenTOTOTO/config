[user]
  name = EverSeenTOTOTO
  email = everseenflash@163.com
[core]
  excludefile = ~/.gitignore
  autocrlf = input
  safecrlf = true
  editor = vim
[color]
    ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold # line info
  old = red # deletions
  new = green # additions
[color "status"]
  added = green
  changed = red
  untracked = yellow
[alias]
  ai = add -i
  c = commit -m
  cln = clone --recursive
  # Switch to a branch, creating it if necessary.
  # Show verbose output about tags, branches or remotes
  tags = tag -l
  branches = branch --all
  remotes = remote --verbose
  # List aliases.
  aliases = config --get-regexp aliasgo = "!f() { git checkout -b \"$1\" 2> /dev/null || git checkout \"$1\"; }; f"
  po = push origin
  pu = push upstream
  cp = cherry-pick
  st = status --column -sb
  su = stash -u
  sk = stash -k
  ll = log --pretty=oneline -n 20 --graph --abbrev-commit --decorate --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
[filter "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge --skip
  process = git-lfs filter-process
[branch]
  sort = -committerdate
[commit]
  gpgsign = false
[stash]
  showPatch = 1
  autocorrect = 1
[merge]
  tool = vim
  log = true
  conflictstyle = diff3
[pull]
  ff = only
[push]
  default = simple
[help]
  autocorrect = 1
[init]
  defaultBranch = main
