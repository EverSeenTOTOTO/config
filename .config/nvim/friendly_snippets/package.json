{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": "c",
        "path": "./snippets/c.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/cpp.json"
      },
      {
        "language": [
          "gitcommit",
          "NeogitCommitMessage"
        ],
        "path": "./snippets/gitcommit.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.json"
      },
      {
        "language": "rust",
        "path": "./snippets/rust.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./snippets/shell.json"
      },
      {
        "language": [
          "markdown"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": "java",
        "path": "./snippets/java.json"
      },
      {
        "language": [
          "html",
          "jade",
          "pug",
          "eruby",
          "javascriptreact",
          "htmldjango"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "javascript",
          "typescript",
          "javascriptreact",
          "typescriptreact",
          "vue",
          "svelte"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/vue.json"
      },
      {
        "language": "make",
        "path": "./snippets/make.json"
      }
    ]
  }
}
