{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.json"
      },
      {
        "language": [
          "gitcommit",
          "NeogitCommitMessage"
        ],
        "path": "./snippets/gitcommit.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh"
        ],
        "path": "./snippets/shell.json"
      },
      {
        "language": [
          "markdown"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "html",
          "jade",
          "pug",
          "eruby",
          "javascriptreact",
          "htmldjango"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/vue.json"
      }
    ]
  }
}
